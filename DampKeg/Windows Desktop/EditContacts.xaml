<Window x:Class="DampKeg.EditContact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edit Contact" Height="280" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="45.333,33,0,0" TextWrapping="Wrap" Text="First Name" VerticalAlignment="Top"/>
        <TextBox x:Name="FirstNameField" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="128.916,32,0,0" KeyDown="GoToLastName"/>
        <TextBox x:Name="LastNameField" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="128.916,82.05,0,0" KeyDown="GoToEmailField"/>
        <TextBlock HorizontalAlignment="Left" Margin="45.333,83.05,0,0" TextWrapping="Wrap" Text="Last Name" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="45.333,132,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top"/>
        <TextBox x:Name="EmailField" HorizontalAlignment="Left" Height="23" Margin="128.916,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="EditContactButtonFocus"/>
        <Button x:Name="EditContactButton" Content="Edit Contact" HorizontalAlignment="Left" Margin="112.666,192.333,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
